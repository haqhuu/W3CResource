#include <stdio.h>
#include <math.h>

int main()
{
	printf("+---+--------------+----------------+\n");
	printf("| n | 2 to power n | 2 to power -n  |\n");
	printf("+---+--------------+----------------+\n");
	for (int i = 0; i < 11; i++)
	{
		printf("|%2d |%10d    |%15.12lf |\n", i, (int)pow(2, i), (double)pow(2, -i));
	}		
	printf("+---+--------------+----------------+\n");
	
	return 0;
}
