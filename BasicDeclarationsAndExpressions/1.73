#include <stdio.h>

int main()
{
	int n1, n2;

	printf("Input the first integer:");
	scanf("%d", & n1);
	printf("Input the second integer:");
	scanf("%d", &n2);
		
	if (n2 == 0 || n1 % n2 != 0) 
		printf("%d is NOT a multiple of %d", n1, n2);
	else
		printf("%d is a multiple of %d", n1, n2);
	
	return 0;
}
